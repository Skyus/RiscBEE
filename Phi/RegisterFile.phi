namespace RiscBEE {
    module RegisterFile(
        clk: Input, rst: @resetHigh Input, write: Input,
        Ddata: Input[31..0],
        Adata: Output[31..0], Bdata: Output[31..0]
    ){
        Register[31..0] bank[31..0] = [32{32b0}];
        
        @ posedge clk {
            if (write) {
                bank[Daddress].next = Ddata;
                bank[0].next = 32b0;
            }
        }
    }
}